{
  "name": "funnel-react",
  "version": "1.0.0",
  "description": "A small bookcase to create a funnel with a modern style",
  "author": "xavivzla",
  "license": "MIT",
  "repository": "https://github.com/xavivzla/Funnel-React",
  "main": "dist/index.js",
